@{
    
    var userRole = Context.Request.Cookies["userRole"];
    if (userRole == "Manager")
    {
        TempData["CustomLayout"] = "_Layout";
    }
    else if (userRole == "Cashier")
    {
        TempData["CustomLayout"] = "_CLayout";
    }
    else if (userRole == "Maintainer")
    {
        TempData["CustomLayout"] = "_MLayout";
    }
    else if (userRole == "Transporter")
    {
        TempData["CustomLayout"] = "_TLayout";
    }
}





